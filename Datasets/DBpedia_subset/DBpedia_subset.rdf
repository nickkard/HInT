    <rdf:type rdf:resource="http://dbpedia.org/ontology/Film"/>
    <fdbp:wikiPageUsesTemplate rdf:resource="http://fr.dbpedia.org/resource/Modèle:Date"/>
    <dbo:wikiPageWikiLink rdf:resource="http://fr.dbpedia.org/resource/Farley_Granger"/>
    <fdbp:wikiPageUsesTemplate rdf:resource="http://fr.dbpedia.org/resource/Modèle:Portail"/>
    
    <dbo:wikiPageWikiLink rdf:resource="http://fr.dbpedia.org/resource/Joan_Chandler"/>
